#!/usr/bin/env bash

cd "$(dirname "$0")" && cd ../..

echo "Clearing build caches..." &&
	echo "$ rm -rf build/" &&
	rm -rf build/ &&
	echo "$ rm -rf _build/" &&
	rm -rf _build/ &&
	echo "$ rm -rf .elixir-tools" &&
	rm -rf .elixir-tools &&
	echo "$ rm -rf .elixir_ls" &&
	rm -rf .elixir_ls &&
	echo "...done!" &&
	exit
